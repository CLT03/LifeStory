<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FAFAFA">

    <TextView
        android:id="@+id/tv_title"
        android:layout_width="0dp"
        android:layout_height="@dimen/x88"
        android:background="#ffffff"
        android:gravity="center"
        android:text="设置"
        android:textColor="#191919"
        android:textSize="@dimen/x32"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/iv_back"
        android:layout_width="@dimen/x88"
        android:layout_height="0dp"
        android:paddingLeft="@dimen/x34"
        android:paddingTop="@dimen/x30"
        android:paddingRight="@dimen/x36"
        android:paddingBottom="@dimen/x26"
        android:background="@drawable/ra_white_btn"
        app:layout_constraintBottom_toBottomOf="@+id/tv_title"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/tv_title"
        app:srcCompat="@mipmap/icon_back_black" />

    <TextView
        android:id="@+id/textView42"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/x40"
        android:layout_marginTop="@dimen/x36"
        android:text="账号"
        android:textColor="#666666"
        android:textSize="@dimen/x24"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_title" />


    <TextView
        android:id="@+id/tv_user_info"
        android:layout_width="0dp"
        android:layout_height="@dimen/x88"
        android:layout_marginTop="@dimen/x14"
        android:background="@drawable/ra_white_btn"
        android:gravity="center_vertical"
        android:paddingStart="@dimen/x120"
        android:paddingRight="@dimen/x106"
        android:text="个人资料"
        android:textColor="#191919"
        android:textSize="@dimen/x28"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView42" />

    <ImageView
        android:id="@+id/imageView17"
        android:layout_width="@dimen/x48"
        android:layout_height="@dimen/x48"
        android:layout_marginLeft="@dimen/x50"
        android:padding="@dimen/x6"
        android:src="@mipmap/icon_edit_setting"
        app:layout_constraintBottom_toBottomOf="@+id/tv_user_info"
        app:layout_constraintStart_toStartOf="@+id/tv_user_info"
        app:layout_constraintTop_toTopOf="@+id/tv_user_info" />

    <ImageView
        android:id="@+id/imageView18"
        android:layout_width="@dimen/x48"
        android:layout_height="@dimen/x48"
        android:layout_marginRight="@dimen/x16"
        android:src="@mipmap/icon_enter_ucenter"
        app:layout_constraintBottom_toBottomOf="@+id/tv_user_info"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/tv_user_info" />

    <View
        android:id="@+id/view6"
        android:layout_width="@dimen/x720"
        android:layout_height="@dimen/x2"
        android:background="#F2F2F2"
        app:layout_constraintBottom_toBottomOf="@id/tv_user_info"
        app:layout_constraintEnd_toEndOf="parent" />


    <TextView
        android:id="@+id/tv_change_psw"
        android:layout_width="0dp"
        android:layout_height="@dimen/x88"
        android:background="@drawable/ra_white_btn"
        android:gravity="center_vertical"
        android:paddingStart="@dimen/x120"
        android:paddingRight="@dimen/x106"
        android:text="修改登录密码"
        android:textColor="#191919"
        android:textSize="@dimen/x28"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_user_info" />

    <ImageView
        android:id="@+id/imageView19"
        android:layout_width="@dimen/x48"
        android:layout_height="@dimen/x48"
        android:layout_marginLeft="@dimen/x50"
        android:padding="@dimen/x6"
        android:src="@mipmap/icon_upwd_setting"
        app:layout_constraintBottom_toBottomOf="@+id/tv_change_psw"
        app:layout_constraintStart_toStartOf="@+id/tv_change_psw"
        app:layout_constraintTop_toTopOf="@+id/tv_change_psw" />

    <ImageView
        android:id="@+id/imageView20"
        android:layout_width="@dimen/x48"
        android:layout_height="@dimen/x48"
        android:layout_marginRight="@dimen/x16"
        android:src="@mipmap/icon_enter_ucenter"
        app:layout_constraintBottom_toBottomOf="@+id/tv_change_psw"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/tv_change_psw" />

    <View
        android:id="@+id/view7"
        android:layout_width="@dimen/x720"
        android:layout_height="@dimen/x2"
        android:background="#F2F2F2"
        app:layout_constraintBottom_toBottomOf="@id/tv_change_psw"
        app:layout_constraintEnd_toEndOf="parent" />

    <TextView
        android:id="@+id/textView43"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/x40"
        android:layout_marginTop="@dimen/x36"
        android:text="其他"
        android:textColor="#666666"
        android:textSize="@dimen/x24"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/view7" />

    <TextView
        android:id="@+id/tv_help"
        android:layout_width="0dp"
        android:layout_height="@dimen/x88"
        android:layout_marginTop="@dimen/x14"
        android:background="@drawable/ra_white_btn"
        android:gravity="center_vertical"
        android:paddingStart="@dimen/x120"
        android:paddingRight="@dimen/x106"
        android:text="帮助与反馈"
        android:textColor="#191919"
        android:textSize="@dimen/x28"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView43" />

    <ImageView
        android:id="@+id/imageView21"
        android:layout_width="@dimen/x48"
        android:layout_height="@dimen/x48"
        android:layout_marginLeft="@dimen/x50"
        android:padding="@dimen/x6"
        android:src="@mipmap/icon_help_setting"
        app:layout_constraintBottom_toBottomOf="@+id/tv_help"
        app:layout_constraintStart_toStartOf="@+id/tv_help"
        app:layout_constraintTop_toTopOf="@+id/tv_help" />

    <ImageView
        android:id="@+id/imageView22"
        android:layout_width="@dimen/x48"
        android:layout_height="@dimen/x48"
        android:layout_marginRight="@dimen/x16"
        android:src="@mipmap/icon_enter_ucenter"
        app:layout_constraintBottom_toBottomOf="@+id/tv_help"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/tv_help" />

    <View
        android:id="@+id/view8"
        android:layout_width="@dimen/x720"
        android:layout_height="@dimen/x2"
        android:background="#F2F2F2"
        app:layout_constraintBottom_toBottomOf="@id/tv_help"
        app:layout_constraintEnd_toEndOf="parent" />

    <TextView
        android:id="@+id/tv_update"
        android:layout_width="0dp"
        android:layout_height="@dimen/x88"
        android:background="@drawable/ra_white_btn"
        android:gravity="center_vertical"
        android:paddingStart="@dimen/x120"
        android:paddingRight="@dimen/x106"
        android:text="版本更新"
        android:textColor="#191919"
        android:textSize="@dimen/x28"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_help" />

    <ImageView
        android:id="@+id/imageView23"
        android:layout_width="@dimen/x48"
        android:layout_height="@dimen/x48"
        android:layout_marginLeft="@dimen/x50"
        android:padding="@dimen/x6"
        android:src="@mipmap/icon_update_setting"
        app:layout_constraintBottom_toBottomOf="@+id/tv_update"
        app:layout_constraintStart_toStartOf="@+id/tv_update"
        app:layout_constraintTop_toTopOf="@+id/tv_update" />

    <ImageView
        android:id="@+id/imageView24"
        android:layout_width="@dimen/x48"
        android:layout_height="@dimen/x48"
        android:layout_marginRight="@dimen/x16"
        android:src="@mipmap/icon_enter_ucenter"
        app:layout_constraintBottom_toBottomOf="@+id/tv_update"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/tv_update" />

    <TextView
        android:id="@+id/textView46"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginRight="@dimen/x10"
        android:text="0.0.0.1"
        android:textColor="#b7b7b7"
        android:textSize="@dimen/x28"
        app:layout_constraintTop_toTopOf="@+id/tv_update"
        app:layout_constraintEnd_toStartOf="@+id/imageView24"
        app:layout_constraintBottom_toBottomOf="@+id/tv_update" />

    <View
        android:id="@+id/view9"
        android:layout_width="@dimen/x720"
        android:layout_height="@dimen/x2"
        android:background="#F2F2F2"
        app:layout_constraintBottom_toBottomOf="@id/tv_update"
        app:layout_constraintEnd_toEndOf="parent" />

    <TextView
        android:id="@+id/tv_about"
        android:layout_width="0dp"
        android:layout_height="@dimen/x88"
        android:background="@drawable/ra_white_btn"
        android:gravity="center_vertical"
        android:paddingStart="@dimen/x120"
        android:paddingRight="@dimen/x106"
        android:text="关于生活故事"
        android:textColor="#191919"
        android:textSize="@dimen/x28"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_update" />

    <ImageView
        android:id="@+id/imageView25"
        android:layout_width="@dimen/x48"
        android:layout_height="@dimen/x48"
        android:layout_marginLeft="@dimen/x50"
        android:padding="@dimen/x6"
        android:src="@mipmap/icon_about_setting"
        app:layout_constraintBottom_toBottomOf="@+id/tv_about"
        app:layout_constraintStart_toStartOf="@+id/tv_about"
        app:layout_constraintTop_toTopOf="@+id/tv_about" />

    <ImageView
        android:id="@+id/imageView26"
        android:layout_width="@dimen/x48"
        android:layout_height="@dimen/x48"
        android:layout_marginRight="@dimen/x16"
        android:src="@mipmap/icon_enter_ucenter"
        app:layout_constraintBottom_toBottomOf="@+id/tv_about"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/tv_about" />

    <View
        android:id="@+id/view10"
        android:layout_width="@dimen/x720"
        android:layout_height="@dimen/x2"
        android:background="#F2F2F2"
        app:layout_constraintBottom_toBottomOf="@id/tv_about"
        app:layout_constraintEnd_toEndOf="parent" />


</android.support.constraint.ConstraintLayout>