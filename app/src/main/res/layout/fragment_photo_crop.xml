<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <View
        android:id="@+id/view40"
        android:layout_width="wrap_content"
        android:layout_height="@dimen/x204"
        android:background="@color/colorWhite"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageView
        android:id="@+id/iv_back6"
        android:layout_width="@dimen/x88"
        android:layout_height="@dimen/x88"
        android:layout_marginStart="@dimen/x42"
        android:background="@drawable/ra_white_btn"
        android:paddingTop="@dimen/x26"
        android:paddingBottom="@dimen/x26"
        android:src="@mipmap/icon_editimg_turnleft"
        app:layout_constraintBottom_toBottomOf="@+id/iv_back4"
        app:layout_constraintStart_toEndOf="@+id/iv_back4"
        app:layout_constraintTop_toTopOf="@+id/iv_back4" />

    <ImageView
        android:id="@+id/iv_back7"
        android:layout_width="@dimen/x88"
        android:layout_height="@dimen/x88"
        android:background="@drawable/ra_white_btn"
        android:paddingTop="@dimen/x26"
        android:paddingBottom="@dimen/x26"
        android:src="@mipmap/icon_editimg_turnright"
        app:layout_constraintBottom_toBottomOf="@+id/iv_back4"
        app:layout_constraintStart_toEndOf="@+id/iv_back6"
        app:layout_constraintTop_toTopOf="@+id/iv_back4"
        app:layout_constraintVertical_bias="0.538" />

    <ImageView
        android:id="@+id/iv_back8"
        android:layout_width="@dimen/x88"
        android:layout_height="@dimen/x88"
        android:background="@drawable/ra_white_btn"
        android:paddingTop="@dimen/x22"
        android:paddingBottom="@dimen/x22"
        android:src="@mipmap/icon_editimg_vertical_flip"
        app:layout_constraintBottom_toBottomOf="@+id/iv_back4"
        app:layout_constraintStart_toEndOf="@+id/iv_back7"
        app:layout_constraintTop_toTopOf="@+id/iv_back4"
        app:layout_constraintVertical_bias="0.538" />

    <ImageView
        android:id="@+id/iv_back9"
        android:layout_width="@dimen/x88"
        android:layout_height="@dimen/x88"
        android:background="@drawable/ra_white_btn"
        android:paddingTop="@dimen/x22"
        android:paddingBottom="@dimen/x22"
        android:src="@mipmap/icon_editimg_horizontal_flip"
        app:layout_constraintBottom_toBottomOf="@+id/iv_back4"
        app:layout_constraintStart_toEndOf="@+id/iv_back8"
        app:layout_constraintTop_toTopOf="@+id/iv_back4"
        app:layout_constraintVertical_bias="0.538" />

    <SeekBar
        android:id="@+id/timeline"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:layout_marginBottom="8dp"
        android:focusable="true"
        android:max="40"
        android:maxHeight="4.0dip"
        android:minHeight="2.0dip"
        android:paddingLeft="16.0dip"
        android:paddingRight="16.0dip"
        android:progress="20"
        android:progressDrawable="@drawable/seekbar"
        android:thumb="@mipmap/icon_progress_block"
        app:layout_constraintBottom_toTopOf="@+id/iv_back6"
        app:layout_constraintEnd_toStartOf="@+id/textView72"
        app:layout_constraintStart_toEndOf="@+id/textView41"
        app:layout_constraintTop_toTopOf="@+id/view40" />

    <View
        android:id="@+id/iv_back4"
        android:layout_width="@dimen/x160"
        android:layout_height="@dimen/x60"
        android:layout_marginStart="@dimen/x92"
        android:layout_marginBottom="8dp"
        android:background="@drawable/r30_gray_btn"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageView
        android:id="@+id/iv_back5"
        android:layout_width="@dimen/x28"
        android:layout_height="@dimen/x28"
        android:layout_marginStart="@dimen/x38"
        android:background="@drawable/transparent"
        android:src="@mipmap/icon_reset"
        app:layout_constraintBottom_toBottomOf="@+id/iv_back4"
        app:layout_constraintStart_toStartOf="@+id/iv_back4"
        app:layout_constraintTop_toTopOf="@+id/iv_back4" />



    <TextView
        android:id="@+id/textView40"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/x8"
        android:text="重置"
        android:textColor="@color/colorWhite"
        android:textSize="@dimen/x26"
        app:layout_constraintBottom_toBottomOf="@+id/iv_back4"
        app:layout_constraintStart_toEndOf="@+id/iv_back5"
        app:layout_constraintTop_toTopOf="@+id/iv_back4" />

    <View
        android:id="@+id/view42"
        android:layout_width="wrap_content"
        android:layout_height="0.5dp"
        android:background="#F7F7F7FF"
        app:layout_constraintBottom_toTopOf="@+id/iv_back2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView41"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/x24"
        android:text="-20"
        app:layout_constraintBottom_toTopOf="@+id/iv_back6"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/view40" />

    <TextView
        android:id="@+id/textView72"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="@dimen/x24"
        android:layout_marginBottom="8dp"
        android:text="20"
        app:layout_constraintBottom_toTopOf="@+id/iv_back6"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/view40" />

    <com.vivwe.base.ui.imageview.CropImageView
        android:id="@+id/eiv_content"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:text="TextView"
        app:layout_constraintBottom_toTopOf="@+id/view40"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</android.support.constraint.ConstraintLayout>