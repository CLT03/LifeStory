<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/root_layout"
    android:background="#66acacac"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <View
        android:id="@+id/view22"
        android:layout_width="@dimen/x220"
        android:layout_height="@dimen/x220"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="8dp"
        android:background="@color/transparent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <com.mbs.sdk.ui.refresh.GifView
        android:id="@+id/util_loading_pgb"
        android:layout_width="@dimen/x100"
        android:layout_height="@dimen/x100"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="@dimen/x32"
        app:gif="@raw/zl_loading"
        app:layout_constraintEnd_toEndOf="@+id/view22"
        app:layout_constraintStart_toStartOf="@+id/view22"
        app:layout_constraintTop_toTopOf="@+id/view22" />

    <TextView
        android:id="@+id/dialog_loading_message_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:gravity="center"
        android:text="@string/prompt_loading"
        android:textColor="#3c3c3c"
        android:textSize="@dimen/x24"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.501"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/util_loading_pgb" />



</android.support.constraint.ConstraintLayout>