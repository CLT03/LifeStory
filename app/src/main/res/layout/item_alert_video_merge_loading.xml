<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:background="#80000000"
    android:layout_height="match_parent">

    <View
        android:id="@+id/view14"
        android:layout_width="@dimen/x280"
        android:layout_height="@dimen/x280"
        android:background="@drawable/r8_black_video_merge_bg"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.502"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tv_progress"
        android:layout_width="@dimen/x72"
        android:layout_height="@dimen/x42"
        android:layout_marginTop="8dp"
        android:background="@mipmap/icon_video_merge_loading_label"
        android:gravity="center_horizontal"
        android:text="0%"
        android:textColor="@color/colorWhite"
        android:textSize="@dimen/x26"
        app:layout_constraintBottom_toTopOf="@+id/view14"
        app:layout_constraintEnd_toEndOf="@+id/view14"
        app:layout_constraintStart_toStartOf="@+id/view14"
        app:layout_constraintTop_toTopOf="@+id/view14" />

    <ImageView
        android:id="@+id/imageView29"
        android:layout_width="@dimen/x160"
        android:layout_height="@dimen/x160"
        android:layout_marginTop="@dimen/x32"
        android:contentDescription="TODO"
        android:src="@mipmap/icon_video_merge_loading"
        app:layout_constraintEnd_toEndOf="@+id/view14"
        app:layout_constraintStart_toStartOf="@+id/view14"
        app:layout_constraintTop_toTopOf="@+id/view14" />

    <com.vivwe.base.ui.CircleBarView
        android:id="@+id/cbv_progress"
        android:layout_width="@dimen/x164"
        android:layout_height="@dimen/x164"
        app:layout_constraintBottom_toBottomOf="@+id/imageView29"
        app:layout_constraintEnd_toEndOf="@+id/imageView29"
        app:layout_constraintStart_toStartOf="@+id/imageView29"
        app:layout_constraintTop_toTopOf="@+id/imageView29" />

    <TextView
        android:id="@+id/textView78"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:layout_marginBottom="8dp"
        android:text="视频合成中"
        android:textColor="@color/colorWhite"
        android:textSize="@dimen/x34"
        app:layout_constraintBottom_toBottomOf="@+id/view14"
        app:layout_constraintEnd_toEndOf="@+id/view14"
        app:layout_constraintStart_toStartOf="@+id/view14"
        app:layout_constraintTop_toBottomOf="@+id/imageView29" />


</android.support.constraint.ConstraintLayout>